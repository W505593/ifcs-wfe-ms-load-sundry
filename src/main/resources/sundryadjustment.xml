            <ns1:GLDescription>{data($sundryTransaction/ns0:glAccountCodes/ns0:description)}</ns1:GLDescription>
        </ns2:SundryAdjustment>
    </ns2:LoadSundryAdjustmentTransaction>
    <ns2:LoadSundryAdjustmentTransaction>
        <ns2:SundryAdjustment>
            <ns1:CustomerAccountNumber>{data($sundryTransaction/ns0:accountNo)}</ns1:CustomerAccountNumber>
            <ns1:AdjustmentTypeCode>{data($sundryTransaction/ns0:adjustmentTypeCode)}</ns1:AdjustmentTypeCode>
            <ns1:EffectiveOn>{data($sundryTransaction/ns0:postedAt)}</ns1:EffectiveOn>
